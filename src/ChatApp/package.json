{
  "version": "1.0.0",
  "name": "asp.net",
  "private": true,
  "scripts": {
    "clean": "npm-run-all clean:*",
    "clean:js": "cleandir wwwroot/js",
    "clean:css": "cleandir wwwroot/css",
    "clean:lib": "cleandir wwwroot/lib",
    "make": "npm-run-all build copy",
    "make:dev": "npm-run-all build:dev copy",
    "build": "npm-run-all build:css build:release_js",
    "build:dev": "npm-run-all build:css build:js",
    "build:js": "cleandir wwwroot/js && grunt typings && webpack",
    "build:css": "cleandir wwwroot/css && grunt build",
    "build:release_js": "cleandir wwwroot/js && grunt typings && webpack --config webpack.production.js",
    "build:typings": "grunt typings",
    "copy": "npm-run-all clean:lib copy:*",
    "copy:bootstrap": "cpx \"node_modules/bootstrap/dist/**/*\" ./wwwroot/lib/bootstrap/dist",
    "copy:jquery": "cpx \"node_modules/jquery/dist/*.js\" ./wwwroot/lib/jquery/dist/",
    "copy:jquery-validation": "cpx \"node_modules/jquery-validation/dist/**/*.js\" ./wwwroot/lib/jquery-validation/dist",
    "copy:jquery-validation-unobtrusive": "cpx \"node_modules/jquery-validation-unobtrusive/*.js\" ./wwwroot/lib/jquery-validation-unobtrusive/"
  },
  "devDependencies": {
    "clean-dir": "^1.0.3",
    "cpx": "^1.5.0",
    "glob": "^7.1.3",
    "grunt": "^1.0.3",
    "grunt-cli": "^1.3.1",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-less": "^2.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "npm-run-all": "^4.1.3",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "typings": "^2.1.1",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@types/angular-ui-bootstrap": "^0.13.47",
    "@types/angularjs": "1.5.5-alpha",
    "@types/jquery": ">=3.0.0",
    "angular": "1.5.5",
    "angular-marked": "^1.2.2",
    "angular-ui-bootstrap": "^2.5.6",
    "bootstrap": "^3.3.6",
    "jquery": ">=3.0.0",
    "jquery-validation": "^1.18.0",
    "jquery-validation-unobtrusive": "^3.2.11",
    "jquery.cookie": "^1.4.1",
    "marked": "^0.5.1"
  }
}
